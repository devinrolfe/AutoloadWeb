{
  "manifest_version": 2,

  "name": "Autoload Web",
  
  "options_ui": {
   	"page": "src/view/options.html"
  },

  "options_page": "options.html",
  "description": "This extension loads websites automatically with a click of a button.",
  "version": "1.1.0",

  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": { 
  	"16": "images/autoLoadWebPic.png",
    "48": "images/autoLoadWebPic48.png",
    "128": "images/autoLoadWebPic128.png" },
  "browser_action": {
    "default_icon": "images/autoLoadWebPic.png",
    "default_title": "Autoload Web",
    "default_popup": "src/view/autoloadWeb_Main.html"
  }
} 